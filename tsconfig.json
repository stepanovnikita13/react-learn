{
	"compilerOptions": {
		"target": "ES6",
		"module": "ES6",
		"moduleResolution": "node",
		"lib": [
			"DOM",
			"DOM.Iterable",
			"ES6",
			"ScriptHost"
		],
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"allowJs": true,
		"jsx": "react-jsx",
		"strict": true,
		"noEmit": true,
	},
	"include": [
		"src"
	]
}